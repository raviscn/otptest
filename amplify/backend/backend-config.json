{
  "function": {
    "otptestCreateAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "otptestDefineAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "otptestPreSignup": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "otptestVerifyAuthChallengeResponse": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {
    "otptest": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "otptestCreateAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "otptestDefineAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "otptestPreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "otptestVerifyAuthChallengeResponse",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": true,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "PHONE_NUMBER"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "PHONE_NUMBER"
        ]
      }
    }
  }
}